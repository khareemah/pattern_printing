#include <stdio.h>

/**
* main - main fuction
*  Return: nothing
* 
**/
 
// print the pattern below
// *
// **
// ***
// ****
// *****


int main(void)
{
    int i, j;
    
    for (i = 1; i <= 5; i++)
    {
      for (j = 0; j < i; j++)
      {
        printf("*");
      }
      printf("\n");
    }
}
